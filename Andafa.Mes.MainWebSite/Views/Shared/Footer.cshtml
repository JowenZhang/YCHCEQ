<div class="text-center footer">
    <text onclick="sthFun()">天津安达发科技有限公司</text>
    <br />
    <text>CopyRight &copy; @((DateTime.Now.Year - 2).ToString())-@DateTime.Now.Year</text>
    <br />
    <input id="val" hidden="hidden" type="number" value="1" />
    <div id="developer" hidden="hidden"></div>
</div>
<script type="text/javascript">
    function sthFun() {
        var val = $('#val').val();
        var v1 = parseInt(val);
        if (v1 == 10) {
            $.getJSON('/Home/GetDevelop', {}, function (data) {
                var obj = $('<div class="text-center text-danger"><text>开发商：' + data.Company + '</text><br /><text>联系：' + data.Contact + '</text></div>');
                $('#developer').empty();
                $('#developer').append(obj);
                $('#developer').show();
            });
            v1 = 1;
        }
        else {
            v1 = v1 + 1;
            $('#developer').empty();
            $('#developer').hide();
        }
        $('#val').val(v1);
    }
</script>