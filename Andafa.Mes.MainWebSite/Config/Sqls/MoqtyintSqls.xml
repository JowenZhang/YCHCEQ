<sqls>
	<sql id="Index">
		<![CDATA[
			SELECT 
				count
			FROM moqtyint;
		]]>
	</sql>
	<sql id="Read">
		<![CDATA[
			SELECT
				count
			FROM moqtyint;
		]]>
	</sql>
	<sql id="Create">
		<![CDATA[
			INSERT INTO moqtyint 
			(count)
			VALUES (@count);
		]]>
	</sql>
	<sql id="Update">
		<![CDATA[
			UPDATE moqtyint SET 
			count = @count;
		]]>
	</sql>
	<sql id="Delete">
		<![CDATA[
			DELETE FROM moqtyint;
		]]>
	</sql>
