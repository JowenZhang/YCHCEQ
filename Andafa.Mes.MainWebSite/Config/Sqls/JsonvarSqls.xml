<?xml version="1.0" encoding="utf-8" ?>
<sqls>
	<sql id="Index">
		<![CDATA[
			SELECT 
				row_to_json
			FROM jsonvar;
		]]>
	</sql>
	<sql id="Read">
		<![CDATA[
			SELECT
				row_to_json
			FROM jsonvar;
		]]>
	</sql>
	<sql id="Create">
		<![CDATA[
			INSERT INTO jsonvar 
			(row_to_json)
			VALUES (@row_to_json);
		]]>
	</sql>
	<sql id="Update">
		<![CDATA[
			UPDATE jsonvar SET 
			row_to_json = @row_to_json;
		]]>
	</sql>
	<sql id="Delete">
		<![CDATA[
			DELETE FROM jsonvar;
		]]>
	</sql>
